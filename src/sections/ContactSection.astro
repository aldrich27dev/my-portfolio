---
import EmailCard from "../contactCards/EmailCard.astro";
import GithubCard from "../contactCards/GithubCard.astro";
import InstagramCard from "../contactCards/InstagramCard.astro";
import LinkedinCard from "../contactCards/LinkedinCard.astro";
import TwitterCard from "../contactCards/TwitterCard.astro";
import YoutubeCard from "../contactCards/YoutubeCard.astro";
import "../styles/contact-section.css";
---

<section class="contact-section" id="contact">
    <h1>Contacto<span class="primary-text">.</span></h1>
    <ul class="contact-links">
        <EmailCard />
        <LinkedinCard />
        <GithubCard />
        <YoutubeCard />
        <InstagramCard />
        <TwitterCard />
    </ul>
</section>
